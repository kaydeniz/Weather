<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

<item>

    <shape android:shape="rectangle">
        <stroke android:color="#bc2a8d"
            android:width="2dp">
        </stroke>
        <corners android:radius="150dp"/>
        <solid android:color="#F1CECA"/>
    </shape>
</item>
<item
    android:left="2dp"
    android:right="2dp"
    android:top="2dp"
    android:bottom="2dp">
    <shape android:shape="rectangle">
        <corners android:radius="150dp"/>
        <solid android:color="#fbad50"/>
    </shape>
</item>

</layer-list>